<div class="row">
  <div class="col-md-6">
    <h3>Listing all users</h3>
  </div>
  <br>
</div>
<div class="row col-md-5">
  <% @users.each do |user| %>
    <div class="row">
      <div class="col-md-4">
        <%= user.designation + ' ' + user.name %>
      </div>
      <div class="col-md-4">
        <%= user.email %>
      </div>
      <div class="col-md-4">
        <div class="col-md-6">
          <%= link_to "Enable"
        </div>
        <div class="col-md-6">
        </div>
      </div>
      <hr>
    </div>
  <% end %>
</div>
