<div class="row">
  <div class="col-md-6">
    <div class="form-group">
      <%= contact_form.label :phone_number, class: 'control-label' %>
      <!-- # [TODO - S] Rails has a telephone_field helper -->
      <%= contact_form.text_field :phone_number, class: 'form-control' %>
    </div>
  </div>
  <div class='col-md-6'>
    <div class='form-group'>
      <!-- # [TODO - S] Rails has an email_field helper -->
      <%= contact_form.label :email, class: 'control-label'%>
      <%= contact_form.text_field :email, class: 'form-control' %>
    </div>
  </div>
</div>
